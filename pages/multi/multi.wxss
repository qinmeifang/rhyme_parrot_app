/**index.wxss**/
.container{
  position: relative;
}
.nav-top{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  padding: 0rpx 5%;
  padding-bottom: 20rpx;
  box-sizing: border-box;
}
.help{
  padding: 0 10rpx;
  height: auto;
  color:#1AAD16;
}
.multi-share{
  text-align: center;
  padding: 10rpx;
  font-size: 28rpx;
  background-color:#1AAD16;
  color: #fff;
  margin-right: 10%;
}
.selected-result{
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  color: #1AAD16;
}
.love-area{
  display: flex;
  justify-content: center;
  width: 100%;
  align-items: center;
  font-size: 24rpx;
}
.love-icon{
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
.search-result{
  display: flex;
  margin-top: 10rpx;
  justify-content: center;
  width: 100%;
}
.multi-item{
  height: 400rpx;
  min-width: 18rpx;
  margin-right: 30rpx;
}
.item-cand{
  height: 60rpx;
}
