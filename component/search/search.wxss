.search {
  width: 100%;
  background-color: #fff;
  margin-right: 5%;
  border-width: 2rpx;
  border-style: solid;
  border-radius: 40rpx;
  height: 70rpx;
  padding: 20rpx;
  box-sizing: border-box;
  display: flex;
  color: #222;
  align-items: center;
}
.search-icon {
  width: 50rpx;
  height: 50rpx;
  float: right;
  margin: 1rpx 5rpx 0 0;
}
.placeholder{
  color: #cacaca;
}