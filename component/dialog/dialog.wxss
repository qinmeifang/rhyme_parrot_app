.dialog-mask{
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  margin-bottom: -100px;
  z-index: 999;
  background: rgba(0, 0, 0, 0.3);
}
.dialog-content{
  width: 80%;
  padding: 60rpx 40rpx;
  border-radius: 8rpx;
  background: #fff;
}